  parameter ZI_MEM_DEPTH = ENABLE_INTER_PHASE_TRANFER_CHECKS ? 
            MAX_OUTSTANDING_REQ : 1;
  
  // This parameter defines the width of pointers used to traverse through
  // memories used for request Vs response tracking.

  parameter ZI_PTR_WDTH = (ZI_MEM_DEPTH < 3)           ? 1  :
                          (2 < ZI_MEM_DEPTH &&
                               ZI_MEM_DEPTH < 5)       ? 2  :
                          (4 < ZI_MEM_DEPTH &&
                               ZI_MEM_DEPTH < 9)       ? 3  :
                          (8 < ZI_MEM_DEPTH &&
                               ZI_MEM_DEPTH < 17)      ? 4  :
                          (16 < ZI_MEM_DEPTH &&
                                ZI_MEM_DEPTH < 33)     ? 5  :
                          (32 < ZI_MEM_DEPTH &&
                                ZI_MEM_DEPTH < 65)     ? 6  :
                          (64 < ZI_MEM_DEPTH &&
                                ZI_MEM_DEPTH < 129)    ? 7  :
                          (128 < ZI_MEM_DEPTH &&
                                 ZI_MEM_DEPTH < 257)   ? 8  :
                          (256 < ZI_MEM_DEPTH &&
                                 ZI_MEM_DEPTH < 513)   ? 9  :
                          (512 < ZI_MEM_DEPTH &&
                                 ZI_MEM_DEPTH < 1025)  ? 10 :
                          (1024 < ZI_MEM_DEPTH &&
                                  ZI_MEM_DEPTH < 2049) ? 11 :
                          (2048 < ZI_MEM_DEPTH &&
                                  ZI_MEM_DEPTH < 4097) ? 12 : 1;
  wire [31:0] pw_ZI_PTR_WDTH = ZI_PTR_WDTH;

  // Basic signals
  input clk;
  input areset_n;
  input [ADDR_WDTH-1:0] maddr;
  input [2:0] mcmd;
  input [DATA_WDTH-1:0] mdata;
  input mdatavalid;
  input mrespaccept;
  input scmdaccept;
  input [DATA_WDTH-1:0] sdata;
  input sdataaccept;
  input [1:0] sresp;

  // Simple Extensions
  input [ADDRSPACE_WDTH-1:0] maddrspace;
  input [ZI_BYTEEN_WDTH-1:0] mbyteen;
  input [ZI_BYTEEN_WDTH-1:0] mdatabyteen;
  input [MDATAINFO_WDTH-1:0] mdatainfo;
  input [REQINFO_WDTH-1:0] mreqinfo;
  input [SDATAINFO_WDTH-1:0] sdatainfo;
  input [RESPINFO_WDTH-1:0] srespinfo;

  // Burst Extensions
  input [ATOMICLENGTH_WDTH-1:0] matomiclength;
  input [BURSTLENGTH_WDTH-1:0] mburstlength;
  input mburstprecise;
  input [2:0] mburstseq;
  input mburstsinglereq;
  input mdatalast;
  input mreqlast;
  input sresplast;

  // Tag Extensions
  input [TAGID_WDTH-1:0] mdatatagid;
  input [TAGID_WDTH-1:0] mtagid;
  input mtaginorder;
  input [TAGID_WDTH-1:0] stagid;
  input staginorder;

  // Thread Extensions
  input [CONNID_WDTH-1:0] mconnid;
  input [THREADID_WDTH-1:0] mdatathreadid;
  input [THREADS-1:0] mthreadbusy;
  input [THREADID_WDTH-1:0] mthreadid;
  input [THREADS-1:0] sdatathreadbusy;
  input [THREADS-1:0] sthreadbusy;
  input [THREADID_WDTH-1:0] sthreadid;

  // Sideband Signals
  input mreset_n;
  input sreset_n;
  input [CONTROL_WDTH-1:0] control;
  input controlbusy;
  input controlwr;
  input [STATUS_WDTH-1:0] status;
  input statusbusy;
  input statusrd;
  
  // base addr for XOR burst
  input [ADDR_WDTH-1:0] base;

  // Parameter configuration ports
  input [2:0] phase_options_group;
  input [6:0] basic_group;
  input [6:0] simple_ext_group;
  input [16:0] burst_ext_group;
  input tag_ext_group;
  input [6:0] thread_ext_group;
  input [5:0] sideband_sig_group;
  input [5:0] cmd_enable_group;

  // Parameters used for state names

  parameter ZI_RESP_IDLE_STATE = 0;
  parameter ZI_RESP_ASSERT_STATE = 1;
  parameter ZI_RESP_ACCEPT_STATE = 2;

  parameter ZI_WR_RESP_IDLE_STATE = 0;
  parameter ZI_WR_RESP_ASSERT_STATE = 1;
  parameter ZI_WR_RESP_ACCEPT_STATE = 2;

  parameter ZI_DATA_IDLE_STATE = 0;
  parameter ZI_DATA_ASSERT_STATE = 1;
  parameter ZI_DATA_ACCEPT_STATE = 2;

  // Internal variables

  reg [1:0] resp_next_state;
  reg [1:0] resp_present_state;
  reg [8*6:0] resp_next_state_string;
  reg [8*6:0] resp_present_state_string;

  reg [1:0] wr_resp_next_state;
  reg [1:0]  wr_resp_present_state;
  reg [8*6:0] wr_resp_next_state_string;
  reg [8*6:0]  wr_resp_present_state_string;

  reg [1:0] data_next_state;
  reg [1:0] data_present_state;
  reg [8*6:0] data_next_state_string;
  reg [8*6:0] data_present_state_string;

  reg [8*4:0] mcmd_string;
  reg [8*4:0] sresp_string;
  reg [8*5:0] mburstseq_string;

  reg cmd_on;
  reg [2:0] r_imcmd;
  reg r_iscmdaccept;
  reg r_imdatavalid;
  reg r_isdataaccept;
  reg [1:0] r_isresp;
  reg r_imrespaccept;
  reg [ADDR_WDTH-1:0] r_imaddr;
  reg [ADDRSPACE_WDTH-1:0] r_imaddrspace;
  reg [ATOMICLENGTH_WDTH-1:0] r_imatomiclength;
  reg [BURSTLENGTH_WDTH-1:0] r_imburstlength;
  reg r_imburstprecise;
  reg [2:0] r_imburstseq;
  reg r_imburstsinglereq;
  reg [ZI_BYTEEN_WDTH-1:0] r_imbyteen;
  reg [CONNID_WDTH-1:0] r_imconnid;
  reg [REQINFO_WDTH-1:0] r_imreqinfo;
  reg r_imreqlast;
  reg [THREADID_WDTH-1:0] r_imthreadid;
  reg [DATA_WDTH-1:0] r_isdata;
  reg [SDATAINFO_WDTH-1:0] r_isdatainfo;
  reg [RESPINFO_WDTH-1:0] r_isrespinfo;
  reg r_isresplast;
  reg [THREADID_WDTH-1:0] r_isthreadid;
  reg [ZI_BYTEEN_WDTH-1:0] r_imdatabyteen;
  reg [DATA_WDTH-1:0] r_imdata;
  reg [MDATAINFO_WDTH-1:0] r_imdatainfo;
  reg r_imdatalast;
  reg [THREADID_WDTH-1:0] r_imdatathreadid;
  reg r_imtaginorder;
  reg r_istaginorder;
  reg [TAGID_WDTH-1:0] r_imtagid;
  reg [TAGID_WDTH-1:0] r_imdatatagid;
