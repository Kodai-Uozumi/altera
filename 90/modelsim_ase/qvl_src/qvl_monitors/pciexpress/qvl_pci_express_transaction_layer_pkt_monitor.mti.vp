  input [7:0] tc_mapped_to_vc_id_6;
  input [7:0] tc_mapped_to_vc_id_7;
  input [7:0] fc_init1_done;
  // Additional gen1 code end
  input extended_tag_field_enable;
  input [2:0] max_payload_size;
