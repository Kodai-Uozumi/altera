  input lock_established_other_device;
  input lock_completed_other_device;
// Additional gen1 code end

  // Output declarations

  output [1:0] tlp_packet_detected;
  output [69:0] tlp_pkt1;
  output [69:0] tlp_pkt2;
  output [1:0] malformed_tlp_detected;

  output [1:0] inc_tlps_with_ecrc;
  output [1:0] inc_tlps_with_digests;

