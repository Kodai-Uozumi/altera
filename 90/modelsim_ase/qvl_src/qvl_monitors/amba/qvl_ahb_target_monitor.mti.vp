  function [7*8:1] hmastlock2name;
    input hmastlock;
    begin
      case (hmastlock)
        ZI_LOCK   : hmastlock2name = "lock";
        ZI_UNLOCK : hmastlock2name = "unlock";
        default   : hmastlock2name = "unknown";
      endcase
    end
  endfunction

  wire [7*8:1] hmastlock_name = hmastlock2name(hmastlock);

  function [9*8:1] interrupted2name;
    input [2:0] interrupted;
    begin
      case (interrupted)
        0                     : interrupted2name = "no";
        ZI_TARGET_ERROR_STATE : interrupted2name = "yes_error";
        ZI_TARGET_RETRY_STATE : interrupted2name = "yes_retry";
        ZI_TARGET_SPLIT_STATE : interrupted2name = "yes_split";
        default               : interrupted2name = "unknown";
      endcase
    end
  endfunction

