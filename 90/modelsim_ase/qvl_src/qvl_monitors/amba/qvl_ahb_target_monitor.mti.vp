  parameter DATA_BUS_WIDTH = 32; /* ##0in value
                                    -var DATA_BUS_WIDTH
                                    -val 8 16 32 64 128 256 512 1024
                                    -name AHB_T27
                                    -clock hclk
                                    -reset 1'b0
                                    -areset !hresetn
                                    -group ahb_target_monitor
  -message "Width of the data bus must be either 8,16,32,64,128,256,512 or 1024"
                                 */
  wire [31:0] pw_DATA_BUS_WIDTH = DATA_BUS_WIDTH;

  parameter NUMBER_OF_MASTERS = 16; /* ##0in range
                                       -var NUMBER_OF_MASTERS
                                       -min 1 -max 16
                                       -name AHB_T28
                                       -clock hclk
                                       -reset 1'b0
                                       -areset !hresetn
                                       -group ahb_target_monitor
                                 -message "Illegal number of masters on the bus"
                                    */
  wire [31:0] pw_NUMBER_OF_MASTERS = NUMBER_OF_MASTERS;

  parameter CANCEL_FOLLOWING_TRANSFER_ON_ERROR_RESPONSE = 0;
  wire [31:0] pw_CANCEL_FOLLOWING_TRANSFER_ON_ERROR_RESPONSE =
                                    CANCEL_FOLLOWING_TRANSFER_ON_ERROR_RESPONSE;

  parameter Over_Constraints_Mode = 0;
  wire [31:0] pw_Over_Constraints_Mode = Over_Constraints_Mode;

  parameter DISABLE_CHKS_ON_IDLE = 0;
  wire [31:0] pw_DISABLE_CHKS_ON_IDLE = DISABLE_CHKS_ON_IDLE;

  input					hselx; /* ##0in known_driven
                                                  -name AHB_T29_hselx
                                                  -clock hclk
                                                  -reset 1'b0
                                                  -areset !hresetn
                                                  -group ahb_target_monitor
                          -message "Control signal, hselx, should not be X or Z"
                                               */

  input [31:0]				haddr; /* ##0in known_driven
                                                  -name AHB_T30_haddr
                                                  -clock hclk
                                                  -reset 1'b0
                                                  -areset !hresetn
                                                  -group ahb_target_monitor
                          -message "Address signal, haddr, should not be X or Z"
                                               */

  input					hwrite; /* ##0in known_driven
                                                   -name AHB_T29_hwrite
                                                   -clock hclk
                                                   -reset 1'b0
                                                   -areset !hresetn
                                                   -group ahb_target_monitor
                         -message "Control signal, hwrite, should not be X or Z"
                                                */

  input [1:0]				htrans; /* ##0in known_driven
                                                   -name AHB_T29_htrans
                                                   -clock hclk
                                                   -reset 1'b0
                                                   -areset !hresetn
                                                   -group ahb_target_monitor
                         -message "Control signal, htrans, should not be X or Z"
                                               */

  input [2:0]				hsize; /* ##0in known_driven
                                                  -name AHB_T29_hsize
                                                  -reset 1'b0
                                                  -areset !hresetn
                                                  -clock hclk
                                                  -group ahb_target_monitor
                          -message "Control signal, hsize, should not be X or Z"
                                               */

  input [2:0]				hburst; /* ##0in known_driven 
                                                   -name AHB_T29_hburst 
                                                   -clock hclk
                                                   -reset 1'b0 
                                                   -areset !hresetn 
                                                   -group ahb_target_monitor
                         -message "Control signal, hburst, should not be X or Z"
                                                */

  input [DATA_BUS_WIDTH-1:0]		hwdata;

  input					hresetn;
  input					hclk;

  input [3:0]				hmaster; /* ##0in known_driven
                                                    -name AHB_T29_hmaster
                                                    -clock hclk
                                                    -reset 1'b0
                                                    -areset !hresetn
                                                    -group ahb_target_monitor
                        -message "Control signal, hmaster, should not be X or Z"
                                                 */

  input					hmastlock; /* ##0in known_driven
                                                      -name AHB_T29_hmastlock
                                                      -clock hclk
                                                      -reset 1'b0
                                                      -areset !hresetn
                                                      -group ahb_target_monitor
                      -message "Control signal, hmastlock, should not be X or Z"
                                                   */

  input					hready_in; /* ##0in known_driven
                                                      -name AHB_T29_hready_in
                                                      -clock hclk
                                                      -reset 1'b0
                                                      -areset !hresetn
                                                      -group ahb_target_monitor
                      -message "Control signal, hready_in, should not be X or Z"
                                                   */

  input					hready_out; /* ##0in known_driven
                                                       -name AHB_T29_hready_out
                                                       -clock hclk
                                                       -reset 1'b0
                                                       -areset !hresetn
                                                       -group ahb_target_monitor
                     -message "Control signal, hready_out, should not be X or Z"
                                                    */

  input [1:0]				hresp; /* ##0in known_driven
                                                  -name AHB_T29_hresp
                                                  -clock hclk
                                                  -reset 1'b0
                                                  -areset !hresetn
                                                  -group ahb_target_monitor
                          -message "Control signal, hresp, should not be X or Z"
                                               */

  input [DATA_BUS_WIDTH-1:0]		hrdata;

  input [NUMBER_OF_MASTERS-1:0]		hsplitx; /* ##0in known_driven
                                                    -name AHB_T29_hsplitx
                                                    -clock hclk
                                                    -reset 1'b0
                                                    -areset !hresetn
                                                    -group ahb_target_monitor
                        -message "Control signal, hsplitx, should not be X or Z"
                                                 */

